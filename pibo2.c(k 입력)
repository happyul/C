#include <stdio.h>
int main()
{

int a=0, b=0, n=0, c=0, k;

scanf("%d",&k);

a=1;  // 첫 번째 피보나치수
n=1;  // n에 1 기록  

b=1;  // 두 번째 피보나치 수
n=n+1;  // n에 2 기록  



while(n<10)
{
c = a+b; //세 번째 피보나치 수 
n= n+1;   // n에 3기록  
if(n==k){
	printf("%d", c); 
	break;}

a = b + c; // 네 번째 피보나치 수 
n = n+ 1;  // n에 4 기록
if(n==k){
	printf("%d", a); 

break;}

b= c + a; //다섯 번째 피보나치 수 
n= n + 1;  // n에 5 기록
if(n==k){
	printf("%d", b); 
break;}


}


 



return 0;

}
