#include <stdio.h>
//두 사람이 번 갈아가며, 1~3사이의 숫자를 말한다.
//말한 숫자를 누적해 나가다가 그 숫자가 31이상 되도록 하는 사람이 지게되는 게임 이다. 

int main()
{
	int a; // 증가하는 숫자 표현 
	int f; //펄스트 라는 의미 첫 번째 사람 

	int s; //세컨드 라는 두 번쨰 사람
	a = 1;
	
	
	scanf("%d", &f); 
  
	while(a<=f)
	{
		printf("%d",a);
		a = a + 1;
	}
	
    scanf("%d", &s); 
	
	
	
	
	

return 0;
}
